#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/run/media/cyb.x/8720ec57-e420-4992-9f53-678accf912c9/Journey/IIT/SDGP/TrustMed/node_modules/.pnpm/@commitlint+cli@20.3.1_@types+node@22.19.6_typescript@5.9.3/node_modules/@commitlint/cli/node_modules:/run/media/cyb.x/8720ec57-e420-4992-9f53-678accf912c9/Journey/IIT/SDGP/TrustMed/node_modules/.pnpm/@commitlint+cli@20.3.1_@types+node@22.19.6_typescript@5.9.3/node_modules/@commitlint/node_modules:/run/media/cyb.x/8720ec57-e420-4992-9f53-678accf912c9/Journey/IIT/SDGP/TrustMed/node_modules/.pnpm/@commitlint+cli@20.3.1_@types+node@22.19.6_typescript@5.9.3/node_modules:/run/media/cyb.x/8720ec57-e420-4992-9f53-678accf912c9/Journey/IIT/SDGP/TrustMed/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/run/media/cyb.x/8720ec57-e420-4992-9f53-678accf912c9/Journey/IIT/SDGP/TrustMed/node_modules/.pnpm/@commitlint+cli@20.3.1_@types+node@22.19.6_typescript@5.9.3/node_modules/@commitlint/cli/node_modules:/run/media/cyb.x/8720ec57-e420-4992-9f53-678accf912c9/Journey/IIT/SDGP/TrustMed/node_modules/.pnpm/@commitlint+cli@20.3.1_@types+node@22.19.6_typescript@5.9.3/node_modules/@commitlint/node_modules:/run/media/cyb.x/8720ec57-e420-4992-9f53-678accf912c9/Journey/IIT/SDGP/TrustMed/node_modules/.pnpm/@commitlint+cli@20.3.1_@types+node@22.19.6_typescript@5.9.3/node_modules:/run/media/cyb.x/8720ec57-e420-4992-9f53-678accf912c9/Journey/IIT/SDGP/TrustMed/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@commitlint/cli/cli.js" "$@"
else
  exec node  "$basedir/../@commitlint/cli/cli.js" "$@"
fi
